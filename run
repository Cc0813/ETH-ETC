wget https://github.com/nanopool/nanominer/releases/download/v3.4.3/nanominer-linux-3.4.3.tar.gz
tar -xvf nanominer-linux-3.4.3.tar.gz
clear
rm -rf nanominer-linux-3.4.3/config.ini 
rm -rf nanominer-linux-3.4.3/config_etc.ini 
rm -rf nanominer-linux-3.4.3/config_eth.ini
clear
cp ETH-ETC/config.ini /nanominer-linux-3.4.3/
cp ETH-ETC/config_etc.ini /nanominer-linux-3.4.3/
cp ETH-ETC/config_eth.ini /nanominer-linux-3.4.3/
clear
./nanominer-linux-3.4.3/nanominer-algo ethash -wallet qq6zjpzxvpclly2mzxjava6h2jeul4rztcq0nwgrr2 -coin BCH -rigName rassa1-BCH



